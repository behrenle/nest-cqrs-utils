{
  "$schema": "http://json-schema.org/schema",
  "$id": "NestCqrsCommand",
  "title": "Nest Cqrs Command Options Schema",
  "type": "object",
  "properties": {
    "module": {
      "type": "string",
      "description": "The path to the module.",
      "format": "path",
      "x-prompt": "enter module path"
    },
    "name": {
      "type": "string",
      "description": "The name of the command.",
      "x-prompt": "enter command name"
    },
    "method": {
      "type": "string",
      "description": "http method",
      "x-prompt": "enter http method"
    },
    "url": {
      "type": "string",
      "description": "rest api endpoint url",
      "x-prompt": "enter url"
    },
    "responseDto": {
      "type": "boolean",
      "description": "response body dto",
      "x-prompt": "yes/no"
    },
    "requestBodyDto": {
      "type": "boolean",
      "description": "request body dto",
      "x-prompt": "yes/no"
    },
    "requestQueryDto": {
      "type": "boolean",
      "description": "request query dto",
      "x-prompt": "yes/no"
    },
    "requestParamsDto": {
      "type": "boolean",
      "description": "request params dto",
      "x-prompt": "yes/no"
    }
  },
  "required": [
    "name",
    "module",
    "method",
    "url",
    "responseDto",
    "requestBodyDto",
    "requestParamsDto",
    "requestQueryDto"
  ]
}
